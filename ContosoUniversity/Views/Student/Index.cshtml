@model IEnumerable<ContosoUniversity.Models.Student>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear nuevo", "Create")
</p>
<table>
    <tr>
        <th>
            Apellido
        </th>
        <th>
            Nombre
        </th>
        <th>
            Fecha de Inscripcion
        </th>
        <th>
            Acciones
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstMidName)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.EnrollmentDate)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.StudentID }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.StudentID }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.StudentID })
        </td>
    </tr>
}

</table>
